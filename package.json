{
  "name": "chakra-data-table",
  "description": "Simple table with great TS",
  "author": "Fran Zekan",
  "version": "1.1.4",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/chakra-confirm.esm.js",
  "scripts": {
    "dev": "tsdx watch",
    "dev:example": "concurrently \"yarn dev\" \"cd example && yarn dev -p 3001\"",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "exit 0",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "@chakra-ui/react": ">=1.2",
    "@emotion/react": ">=11",
    "@emotion/styled": ">=11",
    "framer-motion": ">=2",
    "react": ">=16 || >=0.0.0-experimental-6a589ad71"
  },
  "devDependencies": {
    "@chakra-ui/react": "^1.5.2",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.3.0",
    "@size-limit/preset-small-lib": "^4.6.0",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "babel-jest": "^26.3.0",
    "concurrently": "^6.0.2",
    "framer-motion": "^4.1.6",
    "husky": "^4.3.0",
    "next": "^9.5.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "size-limit": "^4.6.0",
    "tsdx": "^0.14.0",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  },
  "size-limit": [
    {
      "path": "dist/chakra-confirm.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/chakra-confirm.esm.js",
      "limit": "10 KB"
    }
  ]
}
